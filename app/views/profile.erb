<div class="container">
	
	<div class="nudges">
		<h1><%= @user.name %>'s Nudges</h1>
		<ul>
		<% @nudges.each do |nudge| %>
			<li><%= nudge["receiver_name"] %><br>
				<%= nudge["phone_num"] %><br>
				<%= nudge["message"] %><br>
				<%= nudge["datetime"] %><br>
				<%= delete_nudge_button(nudge["id"]) %> 
			</li>
		<% end %>
		</ul>
	</div>
</div>